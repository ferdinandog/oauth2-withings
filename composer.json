{
    "name": "ferdinandog/oauth2-withings",
    "description": "Withings OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Ferdinando Grossi",
            "email": "ferdinando.grossi@gmail.com",
            "homepage": "https://github.com/ferdinandog/"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication",
        "withings"
    ],
    "require": {
        "php": ">=7.4.0",
        "league/oauth2-client": "2.7.*"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.10",
        "friendsofphp/php-cs-fixer": "^3.64",
        "php-parallel-lint/php-parallel-lint": "1.4",
        "phpstan/phpstan": "^1.12"
    },
    "autoload": {
        "psr-4": {
            "WayToHealth\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WayToHealth\\OAuth2\\Client\\Test\\": "tests/src/"
        }
    }
}
